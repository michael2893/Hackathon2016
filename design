public final class Travel {

    /**
     * Private constructor so this utility class cannot be instantiated.
     */
    private Travel() {
    }

  /*@param budget
    *@param cost
    *@param location
    *@param air
    *@param lodging
    *
    *@returns flight
    *@returns hotel
    *@returns cost
    *@returns location
    *
    *@requires buget
    *@requires preferences
    *
    * @ensures <pre>
    * cost = air + lodging
    * cost < budget
    *  </pre>
    */

    private static String Air(String flight) {
    	return flight;
    }

    private static String Lodge(String hotel) {
    	return hotel;

    }

    private static String Location(String destination) {
    	return destination;
    }

    /*@param cost
     *
     *@ensures <pre>
     * cost < budget
     * </pre>
     */

    private static String Locations(String spots){
		while(true){
			int count=0;
			if(spots!=null){
				 count++;
				 Locations(spots);
				 return spots;

			}
		}


    }

    private static void price(double cash, double cost) {
    	if(cash <= cost) {
    		Air();
    		Lodge();
    		Location();
    		System.out.print(");

    	}

    }


    /**
     * Main method.
     *
     * @param args
     *            the command line arguments
     */
    public static void main(String[] args) {
    	Scanner in = new Scanner(System.in);
        System.out.println("Enter budget:");



    }

}
